    &quot;vivo&quot;,
    &quot;tecno&quot;,
    &quot;nokia&quot;,
    &quot;casper&quot;,
    &quot;general mobile&quot;
]

def match = brands.any { brand -&gt; productName.contains(brand) }

if (!match) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(&quot;İlk ürün adı telefon markası içermiyor! Ad: ${productName}&quot;)
